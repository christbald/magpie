<p id="notice"><%= notice %></p>

<h1>Listing Job Monitors</h1>

<table>
  <thead>
    <tr>
      <th>Job</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_monitors.each do |job_monitor| %>
      <tr>
        <td><%= job_monitor.job_id %></td>
        <td><%= job_monitor.user %></td>
        <td><%= link_to 'Show', job_monitor %></td>
        <td><%= link_to 'Edit', edit_job_monitor_path(job_monitor) %></td>
        <td><%= link_to 'Destroy', job_monitor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job monitor', new_job_monitor_path %>

<h1>Available Models</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.name %></td>
        <td><%= link_to 'More', model, :class => "btn btn-mini" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

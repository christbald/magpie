<h1>Available Models</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
    <tr class='clickable-row' data-href='<%= model_path model%>' </tr>
        <td><%= model.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

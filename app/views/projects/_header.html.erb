<div class="panel panel-default">
  <div class="panel-heading">Project: <%= @project.name%></div>
  <div class="panel-body">
    <p>
      <strong>User:</strong>
      <%= @project.user.name %>
    </p>

    <p>
      <strong>Job ID:</strong>
      <%= @project.job_id %>
    </p>

    <p>
      <strong>Name:</strong>
      <%= @project.name %>
    </p>

    <p>
      <strong>Model:</strong>
      <%= File.basename(@project.model) %>
    </p>
    <p>
      <strong>Current Status:</strong>
      <%= @project.status %>
    </p>
  </div>
</div>

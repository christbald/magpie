<div class="panel panel-primary">
  <div class="panel-heading">Project: <%= @project.name%></div>
  <div class="panel-body">
    <p>
      <strong>User:</strong>
      <%= @project.user.name %>
    </p>

    <p>
      <strong>Name:</strong>
      <%= @project.name %>
    </p>

    <p>
      <strong>Model:</strong>
      <%= @project.model ? File.basename(@project.model.name) : 'Deleted model'%>
    </p>
  </div>
</div>
